<nav
  class="x-menu font--accent"
  data-js-class="{{ js_class }}"
  data-x-menu--align="{{ align }}"
  data-x-menu--overlap="false"
  data-x-menu--overlap-parent="{{ overlap_parent }}"
  role="navigation"
  aria-label="{{ 'general.menu' | t }}"
>
  <div class="x-menu--overlap-icon">
    <div class="off-canvas--open" data-off-canvas--open="{{ off_canvas }}">
      {% include 'framework--icons', icon: 'menu' %}
    </div>
  </div>

  <ul class="x-menu--level-1--container">

    {% comment %} Level 1 {% endcomment %}
    
      <li class="x-menu--level-1--link">
          <a
            href="/collections/skincare"
            aria-haspopup="true"
            aria-expanded="false"
            >
            {% comment %} possibly causing shifting of where dropdown appears. investive proprety of aria-controls{% endcomment%}
            <span>
              Skincare
            </span>
            {% include 'framework--icons', icon: 'chevron-down' %}
            {% include 'framework--icons', icon: 'chevron-up' %}
          </a>
        

          {% comment %} Level 2 {% endcomment %}
        
            <div class="x-menu--level-2--container">
              {% comment %}<div class="single-parents"></div> goes here{% endcomment %}
              <div class="x-menu--bg"> 
              <div class="megamenu-container"style="margin-left: 15em;">
                <h1 class="x-menu--bg-title">Skincare</h1>
                <p class="x-menu--bg-description">Healthy skincare for all, thoughtfully curated to suit every budget and every phase of life, skin condition, and lifestyle need.</p>
                {% for product in collections.skincare.products %}
                  {% for tag in product.tags %}
                    {% if tag contains 'megamenu display skincare' %}

                    <div class="x-menu--level-2--products-container">
                      <a href="{{ product.url }}">
                       <img class="lazyload "
             src="{{ product.featured_image | img_url: '200x200' }}"
             data-src="{{ img_url }}"
             data-widths="[80, 120, 200, 300, 450]"
             data-aspectratio="{{ product.featured_image.aspect_ratio }}"
             data-sizes="auto" alt="{{ product.featured_image.alt | escape }}">
                        <div class="caption">
                          <p class="x-menu--level-2-product-title">{{product.vendor}}</p>
                          <p class="x-menu--level-2-product-title">{{product.title}}</p>
                          <div class="grid-view-item__meta">{{ product.price | money }}</div>
                          
                        </div>
                        </a>
                    </div>

                    {% break %}
                    {% endif %}
                  {% endfor %}                       
                  {% endfor %}
                  <br>
                  <a href="/collections/skincare" class="font--button"style="margin-top: 8em; margin-left:5.25em; color: #ffffff !important; margin-top:2em; margin-left: .25em;" target="_self" >Shop Now</a>
                  </div>


              <ul class="x-menu--level-2--list">  
                 <a href="/collections/cleanse">
                <li class="x-menu--level-2--link">
                 
                    <div class="x-menu--level-2--link-container-hydrate">
                     Cleanse
                      
                      <div class="static">
                      <div class="x-menu--level-2--products-hydrate">
                  
                         {% for product in collections.skincare.products %}
                  {% for tag in product.tags %}
                    {% if tag contains 'megamenu display cleanse' %}

                      <a href="{{ product.url }}">
                        <img src="{{ product.featured_image.src  | product_img_url: '200x' }}">
                        <div class="caption">
                          <p class="x-menu--level-2-product-title">{{product.vendor}}</p>
                          <p class="x-menu--level-2-product-title">{{product.title}}</p>
                          <div class="grid-view-item__meta">{{ product.price | money }}</div>
                          
                        </div>
                        </a>

                    {% break %}
                    {% endif %}
                  {% endfor %}                       
                  {% endfor %}
                      </div>
                        </div>
                    </div>
                  
                </li>
                   </a>
                <li class="x-menu--level-2--link">
                  <a href="/collections/hydrate-treat">
                    <div class="x-menu--level-2--link-container-hydrate-2">
                     Hydrate & Treat
                      
                   <div class="static-2">
                    <div class="x-menu--level-2--products-hydrate-2">
                         {% for product in collections.skincare.products %}
                  {% for tag in product.tags %}
                    {% if tag contains 'megamenu display hydrate and treat' %}
                      <a href="{{ product.url }}">
                        <img  src="{{ product.featured_image.src  | product_img_url: '200x' }}">
                        <div class="caption">
                          <p class="x-menu--level-2-product-title">{{product.vendor}}</p>
                          <p class="x-menu--level-2-product-title">{{product.title}}</p>
                          <div class="grid-view-item__meta">{{ product.price | money }}</div>
                        </div>
                        </a>

                    {% break %}
                    {% endif %}
                  {% endfor %}                       
                  {% endfor %}
                      </div>
                       </div>
                      </div>
                  </a>
                </li>
                <li class="x-menu--level-2--link">
                  <a href="/collections/moisturize">
                    <div class="x-menu--level-2--link-container-hydrate-3">
