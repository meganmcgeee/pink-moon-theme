                <li class="x-menu--level-2--link">
                  <a href="/collections/mask-exfoliate">
                    <div class="x-menu--level-2--link-container-hydrate-4">
                     Mask & Exfoliate
                      
                   <div class="static-4">
                    <div class="x-menu--level-2--products-hydrate-4">
                         {% for product in collections.skincare.products %}
                  {% for tag in product.tags %}
                    {% if tag contains 'megamenu display mask and exfoliate' %}
                      <a href="{{ product.url }}">
                        <img  src="{{ product.featured_image.src  | product_img_url: '200x' }}">
